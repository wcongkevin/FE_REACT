{
  "name": "webapp-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "start": "vite --mode loc",
    "start:dv": "vite --mode dv",
    "start:np": "vite --mode np",
    "start:pd": "vite --mode pd",
    "start:qa": "vite --mode qa",
    "build:dv": "vite build --mode dv",
    "build:np": "vite build --mode np",
    "build:pd": "vite build --mode pd",
    "build:qa": "vite build --mode qa",
    "test": "jest",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "github:fortawesome/react-fontawesome",
    "axios": "^1.7.2",
    "bootstrap": "^5.3.3",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "firebase": "^10.12.2",
    "moment": "^2.30.1",
    "multiselect-react-dropdown": "^2.0.25",
    "npm": "^10.8.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.2",
    "react-datepicker": "^7.1.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.2.1",
    "react-loader-spinner": "^6.1.6",
    "react-paginate": "^8.2.0",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.24.1",
    "react-tooltip": "^5.27.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "axios-mock-adapter": "^1.22.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.2.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-environment": "^1.1.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "moduleNameMapper": {
      "\\.(css|less)": "<rootDir>/__mocks__/fileMock.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ]
  }
}
